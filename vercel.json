{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/privacy", "destination": "/privacy.html" },
    { "source": "/terms", "destination": "/terms.html" },
    { "source": "/mobile-app", "destination": "/mobile-app.html" },
    { "source": "/integrations", "destination": "/integrations.html" },
    { "source": "/careers", "destination": "/careers.html" },
    { "source": "/help-center", "destination": "/help-center.html" },
    { "source": "/academy", "destination": "/academy.html" },
    { "source": "/blog", "destination": "/blog.html" },
    { "source": "/free-tools", "destination": "/free-tools.html" }
  ]
}
